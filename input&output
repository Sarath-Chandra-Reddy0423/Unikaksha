#hello World
print("Hello World")

#Hello World with tab
print("Hello World \tHello World")

#Hello World With New Line
print("Hello World \nHello World")

#Interger Input
a=int(input())
print(a)

#Float Input
a=float(input())
print('%2f' %a)

#Character Input
a=input()
print(a)

#String Input
a=str(input())
print(a)

#Ascii-1
a=input()
print(ord(a))

#Ascii-2
a=int(input())
print(chr(a))
